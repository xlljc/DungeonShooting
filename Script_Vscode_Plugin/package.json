{
  "name": "mod-script-support",
  "displayName": "mod-script-support",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ModScriptSupport.helloWorld",
    "onLanguage:ModScriptLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ModScriptSupport.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "ModScript",
        "extensions": [
          ".ms", ".msd"
        ],
        "aliases": [
          "ModScript",
          "modscript"
        ],
        "configuration": "./ModScript-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ModScript",
        "scopeName": "source.ModScript",
        "path": "./syntaxes/ModScript.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
